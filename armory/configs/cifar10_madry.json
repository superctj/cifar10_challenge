    "model": {
        "fit": false,
        "fit_kwargs": {},
        "model_kwargs": {
            "input_shape": [3, 32, 32]
        },
        "module": "arch.wrapper",
        "name": "get_wideresnet",
        "weights_file": "cifar10-atta.ckpt",
        "wrapper_kwargs": {}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.image_classification",
        "name": "ImageClassificationTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/pytorch:0.6.0",
        "external_github_repo": null,
        "gpus": "0",
        "use_gpu": true
    }
